# Development environment
NODE_ENV=development

# Admin Configuration
ADMIN_PUBSUB_PORT=9001
ADMIN_API_PORT=8080
ADMIN_DASHBOARD_PORT=3000

# Coturn Configuration
COTURN_REALM=example.com
COTURN_SECRET=your-secret-key-change-this
COTURN_MIN_PORT=49152
COTURN_MAX_PORT=65535

# TLS Configuration
TLS_CERT_PATH=/etc/ssl/coturn/cert.pem
TLS_KEY_PATH=/etc/ssl/coturn/key.pem

# Logging
LOG_LEVEL=info
LOG_FILE_ENABLED=true
